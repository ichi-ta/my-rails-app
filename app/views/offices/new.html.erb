<% provide(:title, "登録") %>
<h1>新事業所の登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@office) do |f| %>
      <%= f.label :name, "事業所名" %>
      <%= f.text_field :name %>

      <%= f.label :phone_number, "電話番号" %>
      <%= f.telephone_field :phone_number %>

      <%= f.label :address,"住所：" %>
      <%= f.text_field :address %>

      <%= f.submit "登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>