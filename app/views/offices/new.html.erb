<% provide(:title, "登録") %>
<div>
<%= link_to 'back', blogs_path%>
</div>

<h1>新事業所の登録</h1>

<div class="regist-form">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@office) do |f| %>
      <div>事業所名</div>
      <%= f.text_field :name %>

      <div>電話番号</div>
      <%= f.telephone_field :phone_number %>

      <div>住所</div>
      <%= f.text_field :address %>
      <br>
      <%= f.submit "登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>